    <string>/Users/beekhof/Library/Logs/calsync/stderr.log</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/Users/beekhof/go/bin</string>
    </dict>
</dict>
</plist>

